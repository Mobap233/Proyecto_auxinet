<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>VerificarUsuario</servlet-name>
        <servlet-class>Servlet.VerificarUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Modulo_Asfixia</servlet-name>
        <servlet-class>Servlet.Modulo_Asfixia</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Modulo_Epilepsia</servlet-name>
        <servlet-class>Servlet.Modulo_Epilepsia</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Modulo_Botiquin</servlet-name>
        <servlet-class>Servlet.Modulo_Botiquin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Modulo_RCP</servlet-name>
        <servlet-class>Servlet.Modulo_RCP</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>guardarUsuario</servlet-name>
        <servlet-class>Servlet.guardarUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cerrarSesion</servlet-name>
        <servlet-class>Servlet.cerrarSesion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Cuestionarios</servlet-name>
        <servlet-class>Servlet.Cuestionarios</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>actualiza</servlet-name>
        <servlet-class>Servlet.actualiza</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VerificarUsuario</servlet-name>
        <url-pattern>/VerificarUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Modulo_Asfixia</servlet-name>
        <url-pattern>/Modulo_Asfixia</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Modulo_Epilepsia</servlet-name>
        <url-pattern>/Modulo_Epilepsia</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Modulo_Botiquin</servlet-name>
        <url-pattern>/Modulo_Botiquin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Modulo_RCP</servlet-name>
        <url-pattern>/Modulo_RCP</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>guardarUsuario</servlet-name>
        <url-pattern>/guardarUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cerrarSesion</servlet-name>
        <url-pattern>/cerrarSesion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Cuestionarios</servlet-name>
        <url-pattern>/Cuestionarios</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>actualiza</servlet-name>
        <url-pattern>/actualiza</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
